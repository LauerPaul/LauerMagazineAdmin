section.box-typical.scrollable.srcollInit
	table.table.table-striped#categories
		thead.thead-inverse
			tr
				th.text-center
					i.fa.fa-eye
				th Filter Group Name
				th Sort
				th.text-right
					a.btn.btn-success.btn-md(href="category_edit.html")
						i.fa.fa-plus &nbsp;Add
		tbody
			tr
				td.text-center
					i.fa.fa-eye-slash.red
				td
					strong Sneakers
				td.text-center
					span.gray 1
				td
					.btn-group.control-group.pull-right
						a.btn.btn-success.btn-md(href="#")
							i.fa.fa-pencil

						a.btn.btn-danger.btn-md(href="#")
							i.fa.fa-trash-o
			tr
				td.text-center
					i.fa.fa-eye.green
				td
					strong Footwear
				td.text-center
					span.gray 0
				td
					.btn-group.control-group.pull-right
						a.btn.btn-success.btn-md(href="#")
							i.fa.fa-pencil

						a.btn.btn-danger.btn-md(href="#")
							i.fa.fa-trash-o
	nav#paging-table.pagination-datatables.text-center